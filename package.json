{
  "name": "rsvp",
  "version": "0.0.0",
  "jenkins": {
    "dns": {
      "host": "rsvp",
      "domain": "koerber.com.br",
      "resourceGroup": "korber",
      "cname": "obelix.easyware.io"
    },
    "traefik": {
      "resolver": "easywareresolver"
    },
    "nexus": {
      "hostname": "nexus.easyware.io",
      "hostedPort": 8083,
      "groupPort": 8082,
      "imageGroup": "easyware"
    },
    "docker": {
      "labels": "./config/dockerLabels.txt",
      "network": "easyware"
    },
    "sonar": {
      "host": "https://sonar.easyware.io",
      "properties": "./config/sonar.properties"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ssl": "ng serve --ssl --ssl-key ./config/localhost.key --ssl-cert ./config/localhost.crt",
    "build": "ng build",
    "build:prod": "ng build --prod --output-path=dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "sonar": "sonar-scanner -Dproject.settings=./config/sonar.properties"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/common": "^12.0.4",
    "@angular/compiler": "^12.0.4",
    "@angular/core": "^12.0.4",
    "@angular/forms": "^12.0.4",
    "@angular/platform-browser": "^12.0.4",
    "@angular/platform-browser-dynamic": "^12.0.4",
    "@angular/router": "^12.0.4",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.4",
    "@angular/cli": "^12.0.4",
    "@angular/compiler-cli": "^12.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "sonar-scanner": "^3.1.0",
    "tailwindcss": "^2.1.4",
    "typescript": "~4.2.3"
  }
}
